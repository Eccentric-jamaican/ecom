{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "noEmit": false,
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "rootDir": "src",
    "module": "ESNext",
    "target": "ES2019",
    "moduleResolution": "Bundler",
    "skipLibCheck": true,
    "jsx": "react-jsx"
  },
  "include": ["src"]
}
